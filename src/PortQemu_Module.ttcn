module PortQemu_Module {

	import from libnetinet all;

	type record port_qemu_param {
		charstring name,
		charstring val
	}

	type record address {
		ipproto_t ipproto,
		eth_addr_t eth_dst,
		in6_addr_t ip6_dst
	}

	type address qemu_port_addr;

	type port PortQemu message {
		inout octetstring;
		inout port_qemu_param;
	} with { extension "address" }

} with { encode "RAW" }

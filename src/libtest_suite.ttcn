module libtest_suite {

	import from libtest all;
	import from libtcp all;

	function tcp_test(inout cf_t cf) runs on tcp_node
	{
		tcp_node_init(cf);

		bit_set(f.syn);

		tcp_send();

		tcp_recv();

		if (bit_is_set(f.syn) and bit_is_set(f.ack)) {
			setverdict(pass);
		}
	}

	testcase test_libtest_001(inout cf_t cf) runs on tcp_node
	{
		tcp_test(cf);
	}

	control {
		var cf_t cf;
		execute(test_libtest_001(cf));
		execute(test_libtest_001(cf));
	}

} with { encode "RAW" }
